        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.anonymous.lcam"
    },
    "web": {
      "favicon": "./assets/icon-app-lcam.png"
    },
    "extra": {
      "eas": {
        "projectId": "8375a6fc-988d-498a-8a7d-b9bcb3d3ed3e"
